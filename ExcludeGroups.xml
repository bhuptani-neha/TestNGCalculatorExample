<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<!-- Exclude Negative Scenarios that are failing to run positive scenarios and before class method -->
<suite name="All Test Suite">
    <test verbose="2" preserve-order="true" name="Testing Groups">
        <groups>
            <run>
                <exclude name="Negative"/>
            </run>
        </groups>
        <classes>
            <class name="org.example.Calculator_Tests"></class>
        </classes>
    </test>
</suite>

